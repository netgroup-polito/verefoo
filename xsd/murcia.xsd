<!-- ANASTACIA Data aggregation -->
	<complexType name="DataAgreggationConfigurationConditions">
	<complexContent>
			<extension base="ITResource:ConfigurationCondition">
		<sequence>
      		<element maxOccurs="unbounded" minOccurs="1" name="dataAggregationConfigurationCondition" type="ITResource:DataAggregationConfigurationCondition"></element>
		</sequence>
		</extension>
		</complexContent>
