<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema"
	xmlns:ITResource="http://modeliosoft/xsddesigner/a22bd60b-ee3d-425c-8618-beb6a854051a/ITResource.xsd"
	targetNamespace="http://modeliosoft/xsddesigner/a22bd60b-ee3d-425c-8618-beb6a854051a/ITResource.xsd"
	elementFormDefault="qualified"
	xmlns:IoTHoneyNet="http://www.example.org/IoTHoneynetSchema">

	<element name="ITResource" type="ITResource:ITResourceType"/>
	<!-- ANASTACIA policy for orchestration -->
	<element name="ITResourceOrchestration" type="ITResource:ITResourceOrchestrationType"/>
	
	<complexType name="ITResourceOrchestrationType">
		<sequence>
			<element maxOccurs="unbounded" minOccurs="0" name="ITResource" type="ITResource:ITResourceType"></element>
      		<element maxOccurs="1" minOccurs="0" name="priority" type="integer"></element>
			<element maxOccurs="1" minOccurs="0" name="dependencies" type="ITResource:Dependencies"></element>
		</sequence>
		<attribute name="id" type="string"></attribute>
	</complexType>	
	
	<complexType name="ITResourceType">
		<sequence>
			<element maxOccurs="1" minOccurs="1" name="configuration" type="ITResource:Configuration"></element>
				<!-- ANASTACIA policy for orchestration extension -->
			<element maxOccurs="1" minOccurs="0" name="priority" type="integer"></element>
			<element maxOccurs="1" minOccurs="0" name="dependencies" type="ITResource:Dependencies"></element>
			<element maxOccurs="1" minOccurs="0" name="enablerCandidates" type="ITResource:EnablerCandidates"></element>
			<!-- /ANASTACIA policy for orchestration extension -->
		</sequence>
		<attribute name="id" type="string"></attribute>
		<attribute name="orchestrationID" type="string"></attribute>
	</complexType>

<!-- ANASTACIA policy for orchestration extension -->
<!--
	<complexType name="Dependence">
		<sequence>
		  <choice minOccurs="1" maxOccurs="unbounded">
			  <element name="policy" type="ITResource:PolicyDependence"></element>
			  <element name="event" type="ITResource:EventDependence"></element>
			</choice>
		</sequence>
	</complexType>
	-->

	<complexType name="Dependencies">
		<sequence>
		  <element name="dependency" type="ITResource:Dependency" minOccurs="1" maxOccurs="unbounded"></element>
		</sequence>
	</complexType>	
	
		<complexType name="Dependency">
	</complexType>	
	
	
		<complexType name="PolicyDependency">
		<complexContent>
			<extension base="ITResource:Dependency" >
			<sequence>
					<element name="configurationCondition" type="ITResource:PolicyDependencyCondition"></element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	
	    <complexType name="PolicyDependencyCondition">
		<complexContent>
			<extension base="ITResource:ConfigurationCondition" >
			<sequence>
			  <element name="policyID" type="string"></element>
        <element name="status" type="string"></element>
		</sequence>
			</extension>
		</complexContent>
	</complexType>	
	
    <complexType name="EventDependency">
		<complexContent>
			<extension base="ITResource:Dependency" >
			<sequence>
		    <element name="eventID" type="string"></element>
			  <element name="configurationCondition" type="ITResource:ConfigurationCondition"></element>
		</sequence>
			</extension>
		</complexContent>
	</complexType>	
	
		<complexType name="EnablerCandidates">
		<sequence>
			  <element name="enabler" type="string" maxOccurs="unbounded"></element>
		</sequence>
	</complexType>
	

	
<!-- /ANASTACIA policy for orchestration extension -->

