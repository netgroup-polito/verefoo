		</complexContent>
	</complexType>

	<complexType name="DataAggregationAction">
		<complexContent>
			<extension base="ITResource:ConfigurationAction">
				<sequence>
					<element maxOccurs="unbounded" name="dataAggregationActionType" type="ITResource:DataAggregationActionType" />
